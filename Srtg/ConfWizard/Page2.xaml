<Page x:Class="Srtg.ConfWizard.Page2"
      xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
      xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
      xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
      xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
      xmlns:local="clr-namespace:Srtg.ConfWizard"
      mc:Ignorable="d" 
      d:DesignHeight="300" d:DesignWidth="300"
      Title="Page2" Loaded="Page_Loaded">

    <Grid>
        <Label x:Name="label" Content="Choose Interface to monitor" Margin="10,10,10,0" VerticalAlignment="Top" Style="{DynamicResource WizardTitle}"/>
        <ListBox x:Name="lstInts" Margin="10,49,10,41" TabIndex="1"/>
        <Button x:Name="btFinish" Content="Finish" Height="26" VerticalAlignment="Bottom" Margin="0,0,10,10" HorizontalAlignment="Right" Width="91" Click="btFinish_Click" TabIndex="3" IsDefault="True" IsEnabled="False"/>
        <Button x:Name="btBacky" Content="Back" Height="26" VerticalAlignment="Bottom" Margin="0,0,106,10" HorizontalAlignment="Right" Width="91" Click="btBacky_Click" TabIndex="2"/>

    </Grid>
</Page>
